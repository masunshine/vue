  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "element-ui": "^2.9.1",
    "live-server": "^1.2.1",
    "mint-ui": "^2.2.13",
    "moment": "^2.29.2",
    "sass-resources-loader": "^2.0.1",
    "start": "^5.1.0",
    "style-resources-loader": "^1.2.1",
    "vue": "^2.5.21",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "webpack": "^4.35.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "css-loader": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
